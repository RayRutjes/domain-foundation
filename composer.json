{
    "name": "rayrutjes/domain-foundation",
    "description": "Php framework focused on providing the tools to roll out a domain driven project.",
    "keywords": ["ddd", "domain driven", "domain driven design", "event sourcing", "event store", "event bus", "command bus", "framework"],
    "license": "MIT",
    "authors": [
        {
            "name": "Raymond RUTJES",
            "email": "raymond.rutjes@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "RayRutjes\\DomainFoundation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RayRutjes\\DomainFoundation\\Test\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "require": {
        "php": ">=5.6",
        "ramsey/uuid": "^2.8",
        "psr/log": "^1.0",
        "phpmd/phpmd" : "@stable"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    }
}
